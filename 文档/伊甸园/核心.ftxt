                    point.m_pos=self.m_symbol.m_pos.copy()
                    point.m_needed=self
                    self.map(point)
                }
                
                else {
                    self.map(self.m_map)
                }
                self.m_interp=True
                return
            }
            list_have=list_map
            mp=self.m_map
            self.map(self.nextInlist(mp,list_have))
