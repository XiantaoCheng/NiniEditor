    sent_pat(code,pat0) {
        var pat=pat0.replace(/%\[句子[^\[^\]]*\]/,'(.+)');
        var result=code.match(pat);
        if(result==null) {
            return false;
        }
        
        else {
            return true;
        }
    }
    
