            [code,动词]=this.fun_动词(code.slice(1,code.length));
            sent_type=1;
        }
        
        else {
            [code,动词]=this.fun_动词(code);
        }
        if(动词=='') {
            状语='';
            [code,动词]=this.fun_动词(code_save);
        }
        if(动词=='') {
            return [code,''];
        }
        谓语=new NetP("谓语");
        谓语.m_db[1]=动词.m_db[1];
        new NetP('的').con(谓语,动词);
        if(sent_type==1) {
            被=new NetP('被').con('',动词);
            new NetP('的').con(谓语,被);
        }
        if(状语!='') {
            new NetP('的').con(谓语,状语);
            状语.m_db[1].con(动词.m_db[1],0);
        }
        return [code,谓语];
    }

