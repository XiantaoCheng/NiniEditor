[x,y,z]=transform_3D(Source_1_pt,Source_1_an(1),Source_1_an(2),Source_1_an(3),x0,y0,z0);
Beam1_1_X=Source_1_pt(1)+x;
Beam1_1_Y=Source_1_pt(2)+y;
Beam1_1_Z=Source_1_pt(3)+z;

alpha=Source_1_an(1);
beta=Source_1_an(2);
kx=sind(beta)*sind(alpha);
ky=-sind(beta)*cosd(alpha);
kz=cosd(beta);

% BS1
[x,y,z,kx,ky,kz]=Reflection_plane_3D(x,y,z,kx,ky,kz,BS1_1_pt,BS1_1_an);
Beam1_1_X(end+1,:)=x;
Beam1_1_Y(end+1,:)=y;
Beam1_1_Z(end+1,:)=z;

% M1
[x,y,z,kx,ky,kz]=Reflection_plane_3D(x,y,z,kx,ky,kz,M1_1_pt,M1_1_an);
Beam1_1_X(end+1,:)=x;
Beam1_1_Y(end+1,:)=y;
Beam1_1_Z(end+1,:)=z;

% Camera2
[x,y,z]=Plane_intersection(x,y,z,kx,ky,kz,Camera2_1_pt,Camera2_1_an);
Beam1_1_X(end+1,:)=x;
Beam1_1_Y(end+1,:)=y;
Beam1_1_Z(end+1,:)=z;

k1=[kx,ky,kz];
Beam1_1_X=Beam1_1_X';
Beam1_1_Y=Beam1_1_Y';
Beam1_1_Z=Beam1_1_Z';

% Source
w0_2=Source_1_R/2;
lm0_2=800e-9;
x0=0;
y0=0;
z0=0;
[x,y,z]=transform_3D(Source_1_pt,Source_1_an(1),Source_1_an(2),Source_1_an(3),x0,y0,z0);
Beam2_1_X=Source_1_pt(1)+x;
Beam2_1_Y=Source_1_pt(2)+y;
Beam2_1_Z=Source_1_pt(3)+z;

alpha=Source_1_an(1);
beta=Source_1_an(2);
kx=sind(beta)*sind(alpha);
ky=-sind(beta)*cosd(alpha);
kz=cosd(beta);

% M2
[x,y,z,kx,ky,kz]=Reflection_plane_3D(x,y,z,kx,ky,kz,M2_1_pt,M2_1_an);
Beam2_1_X(end+1,:)=x;
Beam2_1_Y(end+1,:)=y;
Beam2_1_Z(end+1,:)=z;

% BS1
[x,y,z,kx,ky,kz]=Reflection_plane_3D(x,y,z,kx,ky,kz,BS1_1_pt,BS1_1_an);
Beam2_1_X(end+1,:)=x;
Beam2_1_Y(end+1,:)=y;
Beam2_1_Z(end+1,:)=z;

% Camera2
[x,y,z]=Plane_intersection(x,y,z,kx,ky,kz,Camera2_1_pt,Camera2_1_an);
Beam2_1_X(end+1,:)=x;
Beam2_1_Y(end+1,:)=y;
Beam2_1_Z(end+1,:)=z;

k2=[kx,ky,kz];
Beam2_1_X=Beam2_1_X';
Beam2_1_Y=Beam2_1_Y';
Beam2_1_Z=Beam2_1_Z';


% interferometer simulation
Dx=diff(Beam2_1_X);
Dy=diff(Beam2_1_Y);
Dz=diff(Beam2_1_Z);
L1=sum(sqrt(Dx.^2+Dy.^2+Dz.^2));

Dx=diff(Beam1_1_X);
Dy=diff(Beam1_1_Y);
Dz=diff(Beam1_1_Z);
L2=sum(sqrt(Dx.^2+Dy.^2+Dz.^2));

orig_1=[Beam1_1_X(end),Beam1_1_Y(end),Beam1_1_Z(end)];
orig_2=[Beam2_1_X(end),Beam2_1_Y(end),Beam2_1_Z(end)];
orig_1=orig_1-k1/norm(k1)*L1;
orig_2=orig_2-k2/norm(k2)*L2;

x0=linspace(-1,1,1000)*w0_1*2;
y0=linspace(-1,1,1000)*w0_1*2;
[X0,Y0]=meshgrid(x0,y0);
Z0=0*X0;
[X,Y,Z]=transform_3D(Camera2_1_pt,Camera2_1_an(1),Camera2_1_an(2),Camera2_1_an(3),X0,Y0,Z0);

% interferometer
E1=GaussianBeam_3D(X,Y,Z,orig_1,k1,lm0_1,w0_1,1,1);
E2=GaussianBeam_3D(X,Y,Z,orig_2,k2,lm0_2,w0_2,1,1);


% KTF crystal
L=2e-2;
Dn2=1.4e-6;

%{
+[M函数](,fig6)
%}
% Magnet
B0=1;
C_n=1;
n=2;
Fn=C_n*(X0-1i*Y0).^(n-1);
Bx=real(Fn);
By=imag(Fn);
B=max(max(sqrt(Bx.^2+By.^2)));
Bx1=Bx./B*B0;


% phase shift
Dphi=2*pi*L*Dn2*Bx1.^2/lm0_1;


E=E1+E2;
I0=abs(E).^2;
E=E1+E2.*exp(1i*Dphi);
I1=abs(E).^2;

subplot(1,2,1)
surf(I1)
view([0,90])
shading interp
axis equal
title('KTF crystal')
xlabel('X [a.u.]')
ylabel('Y [a.u.]')


subplot(1,2,2)
