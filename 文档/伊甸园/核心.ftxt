    isPreDefined() {
        name=self.m_symbol.m_name
        if(name=='[is]' or name=='[eq]' or name=='[那]' or name=='[]') {
            return True
        }
        
        elif(self.isSpaceRelation()) {
            return True
        }
        
        else {
            return False
        }
    }
    
    isSpecialRanger() {
        name=self.m_symbol.m_name
        if(name=='[is]' or name=='[那]') {
            return True
        }
        return False
