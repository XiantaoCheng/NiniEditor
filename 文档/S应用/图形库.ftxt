addpath('Matlab\geometry')
x=pts(1,:);
y=pts(2,:);
[x0,y0,R]=circle_3_pt(x,y);

pt=[x0,y0];

Dx=x-pt(1);
Dy=y-pt(2);
Dx=Dx./sqrt(Dx.^2+Dy.^2);
Dy=Dy./sqrt(Dx.^2+Dy.^2);

an1=sign_1(Dy(1)).*acosd(Dx(1));
an2=sign_1(Dy(end)).*acosd(Dx(end));

if an1-an2<-180
    an1=an1+360;
elseif an1-an2>180
    an1=an1-360;
end

%{
记住"Matlab"
an2
