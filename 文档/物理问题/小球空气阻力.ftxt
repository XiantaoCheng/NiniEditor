%{
+[返回目录]
流场分布::4
+[书签](,流场分布)
(2.5.19)和(2.5.20)
输入::
输出::
%}

a=0.1;
W=1;

q_r=@(theta,r)W*cos(theta).*(1+a^3/2./r.^3-3*a/2./r);
q_t=@(theta,r)-W*sin(theta).*(1-a^3/4./r.^3-3*a/4./r);

% r=linspace(a,1.5*a,20);
% theta=linspace(0,pi,20);
% [R,Theta]=meshgrid(r,theta);
% Z=R.*cos(Theta);
% X=R.*sin(Theta);
z=linspace(-2*a,2*a,40);
x=linspace(0,2*a,20);
[Z,X]=meshgrid(z,x);
R=sqrt(Z.^2+X.^2);
Theta=atan(X./Z);

Q_r=q_r(Theta,R);
Q_t=q_t(Theta,R);

Q_z=Q_r.*cos(Theta)-Q_t.*sin(Theta);
Q_x=Q_r.*sin(Theta)+Q_t.*cos(Theta);

streamline(Z,X,Q_z,Q_x,Z(:,1),X(:,1))
streamline(Z,-X,Q_z,-Q_x,Z(:,1),-X(:,1))
rectangle('Position',[-a,-a,2*a,2*a],'Curvature',[1,1],'FaceColor',[1,1,1]);
axis equal
