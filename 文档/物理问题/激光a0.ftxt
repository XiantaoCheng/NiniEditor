physics_constant;

%E0=24e-3;
%w0=1e-2;
%tau=12e-15;

lambda_0=0.8e-6;
angle=20.1; 

% E0
I0=2*E0/sqrt(pi)/pi/w0^2/tau;
disp(sprintf('I0: %.2e W/cm^2',I0/1e4));

x=linspace(-2*w0,2*w0,100);
y=x;
[X,Y]=meshgrid(x,y);
T=linspace(-2*tau,2*tau,100);

R=sqrt(X.^2+Y.^2);
I_xy=I0*exp(-2*R.^2/w0^2);
I_t=I0*exp(-T.^2/tau^2);

subplot(1,2,1)
surf(X/1e-2,Y/1e-2,I_xy/I0)
view([0,90])
shading interp
axis equal
xlabel('[cm]')
ylabel('[cm]')

subplot(1,2,2)
plot(T/1e-15,I_t/1e4)
xlabel('T [fs]')
ylabel('I [W/cm^2]')

