u_3(i1)=E_0*z(i1)-d_0*E_0*log(2);
u_3(i2)=-E_0*z(i2)-d_0*E_0*log(2);

u_0=-log(2)*E_0*(d_0-d_1);

u=u_1+u_2-u_3+u_0+U_1;


end


