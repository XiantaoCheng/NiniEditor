for i=1:N2
    lm3=1/(1/lm1(j)-1/lm2(i));
    Dk(j,i)=BBO_Dk(lm1(j),lm2(i),lm3,beta0,type_BBO);
end
end

G=(sin(Dk/2*pi*L)./(Dk/2*pi*L)).^2;

