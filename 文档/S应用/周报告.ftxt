#7363, 446:
%{
+[M函数](,fig6)
%}

physics_constant;
C_n=1;
n=1;

x=linspace(-1,1,100);
y=linspace(-1,1,100);
[x,y]=meshgrid(x,y);

Fn=C_n*(x-1i*y).^(n-1);
Bx=real(Fn);
By=imag(Fn);

% Bx=C_n.*(x.^3-3.*y.^2.*x);
% By=C_n.*(y.^3-3.*y.*x.^2);

s1=linspace(-1,1,100);
s2=ones(size(s1));

sx=[s1,s1,s2,-s2];
sy=[s2,-s2,s1,s1];

B0=sqrt(Bx.^2+By.^2);

clf
streamline(x,y,Bx./B0,By./B0,sx,sy)
axis equal
title(sprintf('%d-poles field',n*2))


